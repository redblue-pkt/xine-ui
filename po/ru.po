#: src/xitk/main.c:584
#, c-format
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Использование: xine [ОПЦИИ]... [MRL]\n"

#: src/xitk/main.c:587
#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Вывод номера версии.\n"

#: src/xitk/main.c:588
#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=уровень]     Настройка уровня подробности. По умолчанию - "
"1.\n"

#: src/xitk/main.c:589
#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <файл>          Использование конфигурационного файла вместо "
"используемого по умолчанию.\n"

#: src/xitk/main.c:590
#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <дрв>     Выбор видеодрайвера по его id. Доступные "
"драйверы: \n"

#: src/xitk/main.c:601
#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <дрв>     Выбор аудиодрайвера по его id. Доступные "
"драйверы: \n"

#: src/xitk/main.c:611
#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Выбор канала SPU (субтитров) под номером "
"'#'.\n"

#: src/xitk/main.c:612
#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr ""
"  -a, --audio-channel <#>      Выбор звукового канала под номером '#'.\n"

#: src/xitk/main.c:613
#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [опц]        Воспроизведение при запуске. Может быть:\n"
"                    'f': в полноэкранном режиме.\n"
"                    'h': скрыть GUI (панель и т.п.).\n"
"                    'w': скрыть окно видеосигнала.\n"
"                    'q': выйти после окончания воспроизведения.\n"
"                    'd': извлечь плейлист с DVD. (Устарела. Используйте -s "
"DVD)\n"
"                    'v': извлечь плейлист с VCD. (Устарела. Используйте -s "
"VCD)\n"

#: src/xitk/main.c:621
#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': в полноэкранном режиме xinerama.\n"

#: src/xitk/main.c:623
#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <плагин>     Автосканирование списка воспроизведения из "
"<плагин>\n"

#: src/xitk/main.c:624
#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             Запуск в полноэкранном режиме,\n"

#: src/xitk/main.c:626
#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           Запуск в полноэкранном режиме xinerama (вывод "
"на несколько экранов),\n"

#: src/xitk/main.c:628
#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               Скрыть GUI (панель и т.п.)\n"

#: src/xitk/main.c:629
#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "  -I, --no-gui                 Отключить GUI\n"

#: src/xitk/main.c:630
#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             Скрыть окно видеосигнала\n"

#: src/xitk/main.c:632
#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Отключить поддержку LIRC.\n"

#: src/xitk/main.c:634
#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <класс-или-id>  Использование указанного X11 visual. <класс-"
"или-id>\n"
"                               может быть либо классом visual X11, либо id "
"visual'а.\n"

#: src/xitk/main.c:636
#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Установка собственной карты цветов.\n"

#: src/xitk/main.c:637
#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=опция]        Вывод карты клавиш. Опция:\n"
"                                 'default': вывод таблицы клавиш по "
"умолчанию.\n"
"                                 'lirc': вывод черновика конфигурационного "
"файла .lircrc.\n"
"                                 'remapped': переназначенная пользователем "
"таблица клавиш.\n"
"                                 'file:<имя_файла>': использование "
"<имя_файла> в качестве карты клавиш.\n"
"                                 -если опция не указана, выбирается "
"'default'.\n"

#: src/xitk/main.c:643
#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Включение сервера удаленного управления по "
"сети.\n"

#: src/xitk/main.c:644
#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr ""
"      --network-port           Указание номера сетевого порта сервера.\n"

#: src/xitk/main.c:645
#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Использование корневого окна в качестве окна "
"видеосигнала.\n"

#: src/xitk/main.c:646
#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <ВxШ[+X+Y]>   Установка геометрии окна вывода (в стиле X).\n"

#: src/xitk/main.c:647
#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Окно видеосигнала без рамки.\n"

#: src/xitk/main.c:648
#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        Указание mrl для воспроизведения, когда не "
"используется видеосигнал.\n"
"                                 -может быть использована более одного "
"раза.\n"

#: src/xitk/main.c:650
#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr "  -P, --playlist <имя_файла>   Загрузка файла плейлиста.\n"

#: src/xitk/main.c:651
#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=режим]          Установка режима зацикливания плейлиста (по "
"умолчанию: loop). Режимы:\n"
"                                 'loop': цикл по всему плейлисту.\n"
"                                 'repeat': повтор текущего пункта "
"плейлиста.\n"
"                                 'shuffle': случайный выбор из плейлиста еще "
"не воспроизводившегося пункта\n"
"                                 'shuffle+': эквивалент 'shuffle', но с "
"бесконечным повторением плейлиста.\n"

#: src/xitk/main.c:656
#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  Определение url сервера скинов.\n"

#: src/xitk/main.c:657
#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <mrl> ...      Добавление mrl в плейлист запущенного сеанса "
"(сеанс 0)\n"

#: src/xitk/main.c:658
#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <опция1,опция2,...>\n"
"                               Управление сеансами. Опции:\n"
"                    session=n   выбор номера <n> сеанса,\n"
"                    mrl=m       добавление mrl <m> в плейлист,\n"
"                    audio=c     выбор звукового канала (<c>: 'next' или "
"'prev'),\n"
"                    spu=c       выбор канала spu (<c>: 'next' или 'prev'),\n"
"                    volume=v    настройка громкости,\n"
"                    amp=v       настройка уровня усиления,\n"
"                    loop=m      настройка режима зацикливания (<m>: 'none' "
"'loop' 'repeat' 'shuffle' или 'shuffle+'),\n"
"                    (playlist|pl)=p\n"
"                                 <p> может быть:\n"
"                                   'clear'  очистка плейлиста,\n"
"                                   'first'  воспроизведение первого пункта "
"плейлиста,\n"
"                                   'prev'   воспроизведение предыдущего "
"пункта плейлиста,\n"
"                                   'next'   воспроизведение следующего "
"пункта плейлиста,\n"
"                                   'last'   воспроизведение последнего  "
"пункта плейлиста,\n"
"                                   'load:s' загрузка файла плейлиста <s>,\n"
"                                   'stop'   остановка воспроизведения по "
"окончании текущего.\n"
"                                   'cont'   продолжение воспроизведения по "
"окончании текущего.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#: src/xitk/main.c:680
#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
