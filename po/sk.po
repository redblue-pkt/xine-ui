#, c-format
msgid "No available plugins found of %s type!\n"
msgstr "Žiadny dostupný plugin typu %s sa nenašiel!\n"

#, c-format
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Použitie : xine [VOĽBY]...[MRL]\n"

#, c-format
msgid "OPTIONS are:\n"
msgstr "VOĽBY sú:\n"

#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Vypíše verziu.\n"

#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=úroveň]      Úroveň podrobnosti výpisov. Prednast. 1.\n"

#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <súbor>         Použiť konf.súbor namiesto prednastaveného.\n"

#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv> Výber video ovládača pomocou id. Dostupné "
"ovládače : \n"

#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <dvr> Výber audio ovládača pomocou id. Dostupné "
"ovládače : \n"

#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr "  -u, --spu-channel <#> Výber SPU (titulky) kanála '#'.\n"

#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#> Výber audio kanála '#'.\n"

#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [voľby]      Prehrávať pri štarte. Môže byť nasledované:\n"
"                    'f': na celú obrazovku.\n"
"                    'h': skryť GUI (panel, atď.).\n"
"                    'w': skryť video okno.\n"
"                    'q': skončiť po skončení prehrávania.\n"
"                    'd': získať playlist z DVD. (zastarané. použite -s DVD)\n"
"                    'v': získať playlist z VCD. (zastarané. použite -s VCD)\n"

#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F' : v xinerama celoobrazovkovom režime.\n"

#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr "  -s, --auto-scan <plugin>   stiahnuť playlist z <plugin>\n"

#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             štart v plnoobrazovkovom režime,\n"

#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           štart v xinerama plnoobrazovkovom režime "
"zobrazenie na niekoľko obrazoviek),\n"

#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               skryť GUI (panel, ...)\n"

#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr ""

#, c-format
msgid "      --no-mouse               disable mouse event\n"
msgstr ""

#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             skryť video okno.\n"

#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Vypnúť podporu LIRC.\n"

#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <class-or-id>   Použiť špecifikované X11 zobrazenie. <class-"
"or-id>\n"
"                               je alebo X11 trieda zobr., alebo id "
"zobrazenia .\n"

#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Nainštalovať osobitnú mapu farieb.\n"

#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=voľba]       Zobraziť klávesovú mapu. Voľby sú:\n"
"                                 'default': zobraziť default kl. mapu,\n"
"                                 'lirc': zobraziť návrh .lircrc "
"configuračného súboru.\n"
"                                 'remapped': používateľom zmenená kl. mapa.\n"
"                                 'file:<menosúboru>': použiť <menosúboru> "
"ako kl. mapu.\n"
"                                 -ak nie je zadané, 'default' je zvolené.\n"

#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Povoliť sieťový server diaľkového ovládania.\n"

#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr ""

#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Použiť pracovnú plochu pre zobrazenie videa\n"

#, c-format
msgid "  -W, --wid                    Use a window id.\n"
msgstr "  -W, --wid                    Použiť id okna\n"

#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr "  -G, --geometry <WxH[+X+Y]>   Nastviť výstupné okno (X style).\n"

#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Výstupné okno bez rámu.\n"

#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        Zadať mrl ktoré sa prehráva, keď video výstup "
"nie je použitý.\n"
"                                 -može byť použité viac ako raz.\n"

#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr "  -P, --playlist <menosúboru>    Nahrať playlist.\n"

#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=mode]           Nastaviť mód opakovania (default: loop). Módy "
"sú:\n"
"                                 'loop': opakovať celý playlist.\n"
"                                 'repeat': opakovať aktuálnu položku z "
"playlistu.\n"
"                                 'shuffle': vybrať náhodnú nehranú položku z "
"playlistu\n"
"                                 'shuffle+': ako 'shuffle', ale donekonečna "
"prehráva playlist.\n"

#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  Definovať adresu skin servera.\n"

#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <mrl> ...      Zaradí mrl bežiacej session (session 0)\n"

#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <voľba1,voľba2,...>\n"
"                               Session management. Voľby môžu byť:\n"
"                    session=n   špecifikovať session číslo <n> ,\n"
"                    mrl=m       pridať mrl <m> do playlistu,\n"
"                    audio=c     vybrať audio kanál (<c>: 'next' alebo "
"'prev'),\n"
"                    spu=c       vybrať spu kanál (<c>: 'next' alebo "
"'prev'),\n"
"                    volume=v    nastaviť audio hlasitosť,\n"
"                    amp=v       nastaviť úroveň zosilnenia,\n"
"                    loop=m      nastaviť mód opakovania (<m>: 'none' 'loop' "
"'repeat' 'shuffle' or 'shuffle+'),\n"
"                    (playlist|pl)=p\n"
"                                 <p> môže byť:\n"
"                                   'clear'  vyčistiť playlist,\n"
"                                   'first'  prehrať prvú položku z "
"playlistu,\n"
"                                   'prev'   prehrať predch. položku "
"playlistu,\n"
"                                   'next'   prehrať ďalšiu položku "
"playlistu,\n"
"                                   'last'   prehrať posl. položku "
"playlistu,\n"
"                                   'load:s' nahrať playlist súbor <s>,\n"
"                                   'stop'   zastaviť prehrávanie na konci "
"aktuálneho prehrávania.\n"
"                                   'cont'   pokračovať prehrávanie na konci "
"aktuálneho prehrávania.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           Nezačať automaticky prehrávať (smart mode).\n"

#, c-format
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""
"  -D, --deinterlace [post]...  Deinterlace video výstup. Jeden alebo viac "
"pluginov\n"
"                                 môže byť zadaných, s voliteľnými "
"parametrami.\n"
"                                 Syntax je rovnaká ako pri --post voľbe.\n"

#, c-format
msgid ""
"  -r, --aspect-ratio <mode>    Set aspect ratio of video output. Modes are:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"
msgstr ""
"  -r, --aspect-ratio <mód>    Nastaviť pomer strán video výstupu. Módy sú:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"

#, c-format
msgid ""
"      --broadcast-port <port>  Set port of xine broadcaster (master side)\n"
"                               Slave is started with 'xine slave://address:"
"port'\n"
msgstr ""
"      --broadcast-port <port>  Nastaviť port xine vysielača (strana master)\n"
"                               Slave sa štaruje 'xine slave://adresa:port'\n"

#, c-format
msgid "      --no-logo                Don't display the logo.\n"
msgstr "      --no-logo                Nezobrazovať logo.\n"

#, c-format
msgid "  -E, --no-reload              Don't reload old playlist.\n"
msgstr "  -E, --no-reload              Nenatiahnuť starý playlist.\n"

#, c-format
msgid ""
"      --post <plugin>[:parameter=value][,...][;...]\n"
"                               Load one or many post plugin(s).\n"
"                               Parameters are comma separated.\n"
"                               This option can be used more than one time.\n"
msgstr ""
"      --post <plugin>[:parameter=hodnota][,...][;...]\n"
"                               Nahrať jeden alebo viac post pluginov.\n"
"                               Parametere sú oddelené čiarkou.\n"
"                               Môže byť použité viac ako raz.\n"

#, c-format
msgid "      --disable-post           Don't enable post plugin(s).\n"
msgstr "      --disable-post           Zakázať post plugin(y).\n"

#, c-format
msgid "      --no-splash              Don't display the splash screen.\n"
msgstr "      --no-splash              Nezobrazovať splash screen.\n"

#, c-format
msgid ""
"      --stdctl                 Control xine by the console, using lirc "
"commands.\n"
msgstr ""
"      --stdctl                 Ovládať xine cez konzolu, použitím lirc "
"príkazov.\n"

#, c-format
msgid "  -T, --tvout <backend>        Turn on tvout support. Backend can be:\n"
msgstr ""
"  -T, --tvout <backend>        Zapnuť podporu pre TV. Backend môže byť:\n"

#, c-format
msgid ""
"      --list-plugins [=type]   Display the list of all available plugins,\n"
"                               Optional <type> can be:\n"
"                    audio_out, video_out, demux, input, sub, post,\n"
"                    audio_decoder, video_decoder.\n"
msgstr ""
"      --list-plugins [=typ]   Zobraziť zoznam dostupných pluginov,\n"
"                               Voliteľne <typ> môže byť:\n"
"                    audio_out, video_out, demux, input, sub, post,\n"
"                    audio_decoder, video_decoder.\n"

#, c-format
msgid ""
"      --bug-report [=mrl]      Enable bug report mode:\n"
"                                 Turn on verbosity, gather all output\n"
"                                 messages and write them into a file named\n"
"                                 BUG-REPORT.TXT.\n"
"                                 If <mrl> is given, xine will play the mrl\n"
"                                 then quit (like -pq).\n"
msgstr ""
"      --bug-report [=mrl]      Zapnúť mód hladania chýb:\n"
"                                 Zapnúť verbosity, všetky výstupné\n"
"                                 správy zapísať do súboru\n"
"                                 BUG-REPORT.TXT.\n"
"                                 Ak <mrl> je zadané, xine prehrá mrl\n"
"                                 a ukončí sa (ako -pq).\n"

