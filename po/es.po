#: src/xitk/main.c:138
#, c-format
msgid "Built with xine library %d.%d.%d [%s]-[%s]-[%s].\n"
msgstr "Construído con la biblioteca xine %d.%d.%d [%s]-[%s]-[%s].\n"

#: src/xitk/main.c:141
#, c-format
msgid "Found xine library version: %d.%d.%d (%s).\n"
msgstr "Encontrada versión de biblioteca xine: %d.%d.%d (%s).\n"

#: src/xitk/main.c:156
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Uso: xine [OPTIONS]... [MRL]\n"

#: src/xitk/main.c:159
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Mostrar versión.\n"

#: src/xitk/main.c:160
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Seleccionar id. manejador de vídeo. "
"Disponibles: \n"

#: src/xitk/main.c:169
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Select id. manejador de sonido. "
"Disponibles: \n"

#: src/xitk/main.c:179
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Seleccionar núm. canal SPU (subtítulos).\n"

#: src/xitk/main.c:180
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Seleccionar núm. canal de audio.\n"

#: src/xitk/main.c:181
msgid "  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
msgstr ""
"  -p, --auto-play [opt]        Reproducir al comenzar. Puede seguirlo:\n"

#: src/xitk/main.c:182
msgid "                    'f': in fullscreen mode.\n"
msgstr "                    'f': en modo de pantalla completa.\n"

#: src/xitk/main.c:183
msgid "                    'h': hide GUI (panel, etc.).\n"
msgstr "                    'h': ocultar interfaz (panel, etc.).\n"

#: src/xitk/main.c:184
#, fuzzy
msgid "                    'w': hide video window.\n"
msgstr "  -H, --hide-video             ocultar ventana de vídeo\n"

#: src/xitk/main.c:185
msgid "                    'q': quit when play is done.\n"
msgstr "                    'q': salir cuando acabe la reproducción.\n"

#: src/xitk/main.c:186
msgid ""
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
msgstr ""
"                    'd': capturar lista repr. del DVD. (obsoleto. use -s "
"DVD)\n"

#: src/xitk/main.c:187
msgid ""
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"                    'v': capturar lista repr. del VCD. (obsoleto. use -s "
"VCD)\n"

#: src/xitk/main.c:188
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     buscar automáticamente lista repr. con "
"<plugin>\n"

#: src/xitk/main.c:189
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             empezar en modo de pantalla completa,\n"

#: src/xitk/main.c:190
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               ocultar interfaz (panel, etc.)\n"

#: src/xitk/main.c:191
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             ocultar ventana de vídeo\n"

#: src/xitk/main.c:193
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Desactivar el soporte LIRC.\n"

#: src/xitk/main.c:195
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
msgstr ""
"      --visual <class-or-id>   Usar el visual X11 especificado. <class-or-"
"id>\n"

#: src/xitk/main.c:196
msgid ""
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"                               es o una clase visual X11 o un id. visual.\n"

#: src/xitk/main.c:197
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Instala un mapa de color privado.\n"

#: src/xitk/main.c:198
msgid "      --keymap [option]        Display keymap. Option are:\n"
msgstr "      --keymap [option]        Mostrar mapeo de teclado. Opciones:\n"

#: src/xitk/main.c:199
msgid ""
"                                 'default': display default keymap table,\n"
msgstr ""
"                                 'default': muestra mapeo teclado por "
"defecto,\n"

#: src/xitk/main.c:200
msgid ""
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
msgstr ""
"                                 'lirc': muestra borrador arch. config. ."
"lircrc.\n"

#: src/xitk/main.c:201
msgid ""
"                                 'remapped': user remapped keymap table.\n"
msgstr ""
"                                 'remapped': mapeo tecl. remapeado por "
"usuario.\n"

#: src/xitk/main.c:202
msgid ""
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"                                 -si no se indica opción, se toma "
"'default'.\n"

#: src/xitk/main.c:204
msgid "examples for valid MRLs (media resource locator):\n"
msgstr ""
"ejemplos de MRLs válidos (media resource locator - localizador de medios):\n"

#: src/xitk/main.c:205
msgid "  File:  'path/foo.vob'\n"
msgstr "  File:  'path/foo.vob'\n"

#: src/xitk/main.c:206
msgid "         '/path/foo.vob'\n"
msgstr "         '/path/foo.vob'\n"

#: src/xitk/main.c:207
msgid "         'file://path/foo.vob'\n"
msgstr "         'file://path/foo.vob'\n"

#: src/xitk/main.c:208
msgid "         'fifo://[[mpeg1|mpeg2]:/]path/foo'\n"
msgstr "         'fifo://[[mpeg1|mpeg2]:/]path/foo'\n"

#: src/xitk/main.c:209
msgid "         'stdin://[mpeg1|mpeg2]' or '-' (mpeg2 only)\n"
msgstr "         'stdin://[mpeg1|mpeg2]' o '-' (mpeg2 only)\n"

#: src/xitk/main.c:210
msgid "  DVD:   'dvd://VTS_01_2.VOB'\n"
msgstr "  DVD:   'dvd://VTS_01_2.VOB'\n"

#: src/xitk/main.c:211
msgid "  VCD:   'vcd://<track number>'\n"
msgstr "  VCD:   'vcd://<núm. pista>'\n"

