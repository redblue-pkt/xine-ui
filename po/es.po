#: src/xitk/main.c:586
#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Seleccionar manejador de vídeo por id. "
"Drivers disponibles: \n"

#: src/xitk/main.c:597
#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Seleccionar manejador de sonido por id. "
"Drivers disponibles:\n"

#: src/xitk/main.c:607
#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Seleccionar núm. canal SPU (subtítulos) '#'.\n"

#: src/xitk/main.c:608
#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Seleccionar núm. canal de audio '#'.\n"

#: src/xitk/main.c:609
#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Reproducción desde el inicio. Puede ser "
"seguida por :\n"
"                    'f' : en modo de pantalla completa.\n"
"                    'h' : ocultar IGU (panel, etc.)\n"
"                    'w' : ocultar la ventana de vídeo.\n"
"                    'q' : salir cuando la reproducción termine.\n"
"                    'd' : obtener la lista de lectura desde el DVD. "
"(anticuado, use -s DVD)\n"
"                    'v' : obtener la lista de lectura desde el VCD. "
"(anticuado, use -s VCD)\n"

#: src/xitk/main.c:617
#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': modo a pantalla completa en xinerama.\n"

#: src/xitk/main.c:619
#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     buscar automáticamente lista repr. desde el "
"<plugin> (complemento)\n"

#: src/xitk/main.c:620
#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             empezar en modo de pantalla completa,\n"

#: src/xitk/main.c:622
#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           comenzar en pantalla completa de xinerama "
"(pantalla en varios monitores),\n"

#: src/xitk/main.c:624
#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr ""
"  -g, --hide-gui               ocultar interfaz gráfica (panel, etc.)\n"

#: src/xitk/main.c:625
#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "  -I, --no-gui                 desactivar interfaz gráfica\n"

#: src/xitk/main.c:626
#, c-format
msgid "      --no-mouse               disable mouse event\n"
msgstr "      --no-mouse               desactivar evento de ratónt\n"

#: src/xitk/main.c:627
#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             ocultar ventana de vídeo\n"

#: src/xitk/main.c:629
#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Desactivar el soporte LIRC.\n"

#: src/xitk/main.c:631
#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <classe-ou-nom> Usar la visual X11 especificado. <class-or-"
"id>\n"
"                               Es una clase visual de X11 o un id. de "
"visual\n"

#: src/xitk/main.c:633
#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Instala un mapa de color privado.\n"

#: src/xitk/main.c:634
#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=option]       Muestra el mapa de teclado. Las opciones "
"son:\n"
"                                 'default' : muestra el mapa de teclado "
"original,\n"
"                                 'lirc': muestra el borrador de un archivo "
"de configuración .lircrc.\n"
"                                 'remapped' : muestra las secuencias "
"remapeadas por el usuario.\n"
"                                 'file:<filename>' : utiliza <filename> como "
"mapa de teclado.\n"
"                                 -si ninguna opción es dada, 'default' será "
"seleccionada.\n"

#: src/xitk/main.c:640
#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Activar servidor de control remoto por red.\n"

#: src/xitk/main.c:641
#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr ""
"      --network-port           Especifica puerto del servidor de red.\n"

#: src/xitk/main.c:642
#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Usar ventana raíz como ventana de vídeo.\n"

#: src/xitk/main.c:643
#, c-format
msgid "  -W, --wid                    Use a window id.\n"
msgstr "  -W, --wid                    Usar ID de ventana.\n"

#: src/xitk/main.c:644
#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <WxH[+X+Y]>   Poner geometría de la ventana salida (estilo "
"X).\n"

#: src/xitk/main.c:645
#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Ventana de vídeo sin bordes.\n"

#: src/xitk/main.c:646
#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        Especifica mrl a reproducir cuando no se usa "
"la salida de vídeo.\n"
"                                 -puede ser usada más de una vez.\n"

#: src/xitk/main.c:648
#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr ""
"  -P, --playlist <archivo>     Carga un archivo de lista de reproducción.\n"

#: src/xitk/main.c:649
#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=mode]           Establece el modo de repetición de la lista "
"de reproducción. Los modos son:\n"
"                                 'loop': repite toda la lista.\n"
"                                 'repeat': repite la selección actual.\n"
"                                 'shuffle': selección aleatoria de una "
"entrada todavía no reproducida.\n"
"                                 'shuffle+': igual a shuffle, pero repite "
"indefinidamente la lista de reproducción.\n"

#: src/xitk/main.c:654
#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  Define la url del servidor de pieles.\n"

#: src/xitk/main.c:655
#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <mrl> ...      Encola mrl de una sesión activa (sesión 0)\n"

#: src/xitk/main.c:656
#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <option1, option2,...>\n"
"                               Manejo de sesiones. Las opciones pueden ser:\n"
"                    session=n   especifica el número de sesión <n>,\n"
"                    mrl=m       agrega el mrl <m> a la lista de "
"reproducción,\n"
"\n"
"                    audio=c     selecciona el canal de audio (<c>: 'next' o "
"'prev')\n"
"                    spu=c       seleciona el canal de subtítulo (<c>: 'next' "
"o 'prev')\n"
"                    volume=v    establece el volumen de audio,\n"
"                    amp=v       establece el nivel de amplificación,\n"
"                    loop=m      selecciona el modo de bucle (<m>: 'none' "
"'loop' 'repeat' 'shuffle' o 'shuffle+'),\n"
"                    get_speed   obtiene el valor de velocidad (ver página "
"man para los valores retornados),\n"
"                    (playlist|pl)=p\n"
"                                 <p> puede ser:\n"
"                                   'clear'  limpia la lista de "
"reproducción,\n"
"                                   'first'  reproduce la primera entrada en "
"la lista de reproducción,\n"
"                                   'prev'   reproduce la entrada anterior,\n"
"                                   'next'   reproduce la entrada siguiente,\n"
"                                   'last'   reproduce la última entrada en "
"la lista de reproducción,\n"
"                                   'load:s' carga el archivo <s> como lista "
"de reproducción,\n"
"                                   'stop'   detiene la reproducción al final "
"de la reproducción en curso,\n"
"                                   'cont'   continua la reproduccón al final "
"de la reproducción en curso,\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#: src/xitk/main.c:678
#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           No empezar reproducción automática (modo "
"inteligente).\n"

#: src/xitk/main.c:679
#, c-format
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""
"  -D, --deinterlace [post]...  Desentrelazar la salida de vídeo. Se puede "
"especificar\n"
"                                 uno o varios post-complementos, con "
"parámetros opcionales.\n"
"                                 La sintaxis es idéntica a la opción --"
"post.\n"

#: src/xitk/main.c:682
#, c-format
msgid ""
"  -r, --aspect-ratio <mode>    Set aspect ratio of video output. Modes are:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"
msgstr ""
"  -r, --aspect-ratio <mode>    Establece la relación de aspecto de la salida "
"de vídeo. Los modos son:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'. \n"

#: src/xitk/main.c:685
#, c-format
msgid ""
