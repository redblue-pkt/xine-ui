msgstr "Se requiere el id. del manejador de vídeo para la opción -V\n"

#, c-format
msgid "audio driver id required for -A option\n"
msgstr "Se requiere el id. del manejador de audio para la opción -V\n"

#, c-format
msgid "Bad aspect ratio mode '%s', see xine --help\n"
msgstr "Razón de aspecto '%s' incorrecta, vea xine --help\n"

#, c-format
msgid "Bad port number: %d\n"
msgstr "Mal número de puerto: %d\n"

#, c-format
msgid "Bad geometry '%s', see xine --help\n"
msgstr "Geometría incorrecta '%s', vea xine --help\n"

#, c-format
msgid "Bad loop mode '%s', see xine --help\n"
msgstr "Modo de repetición '%s' incorrecto, vea xine --help\n"

#, c-format
msgid "*** NOTE ***\n"
msgstr "*** NOTA ***\n"

#, c-format
msgid ""
" Bug Report mode: All output messages will be added in BUG-REPORT.TXT file.\n"
msgstr ""
"Modo de reporte de errores: Todos los mensajes de salida serán agregados al "
"fichero BUG-REPORT.TXT.\n"

#, c-format
msgid "window id required for -W option\n"
msgstr "Se requiere el id. de la ventana -W\n"

#, c-format
msgid "invalid argument %d => exit\n"
