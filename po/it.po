#: src/xitk/main.c:456
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=livello]       Imposta il livello di verbosity. Il valore "
"predefinito è 1.\n"

#: src/xitk/main.c:457
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <file>          Usa il file di configurazione indicato invece "
"di quello predefinito.\n"

#: src/xitk/main.c:458
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Seleziona il driver video driver per id. "
"Driver disponibili: \n"

#: src/xitk/main.c:469
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Seleziona il driver audio per id. Driver "
"disponibili: \n"

#: src/xitk/main.c:479
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Seleziona il canale SPU (sottotitolo) '#'.\n"

#: src/xitk/main.c:480
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Seleziona il canale audio '#'.\n"

#: src/xitk/main.c:481
msgid "  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
msgstr ""
"  -p, --auto-play [opt]        Riproduce all'avvio. Può essere seguito da:\n"

#: src/xitk/main.c:482
msgid "                    'f': in fullscreen mode.\n"
msgstr "                    'f': modalità a pieno schermo.\n"

#: src/xitk/main.c:484
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': modalità a pieno schermo con xinerama.\n"

#: src/xitk/main.c:486
msgid "                    'h': hide GUI (panel, etc.).\n"
msgstr "                    'h': nascondi GUI (pannello, etc.).\n"

#: src/xitk/main.c:487
msgid "                    'w': hide video window.\n"
msgstr "                    'w': nascondi finestra video.\n"

#: src/xitk/main.c:488
msgid "                    'q': quit when play is done.\n"
msgstr "                    'q': esci quando la riproduzione è completa.\n"

#: src/xitk/main.c:489
msgid ""
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
msgstr ""
"                    'd': ricava la playlist dal DVD. (deprecato. Usa -s "
"DVD)\n"

#: src/xitk/main.c:490
msgid ""
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"                    'v': ricava la playlist dal VCD. (deprecato. Usa -s "
"VCD)\n"

#: src/xitk/main.c:491
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     scansione automatica playlist dal <plugin>\n"

#: src/xitk/main.c:492
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             avvia in modalità a schermo pieno,\n"

#: src/xitk/main.c:494
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           avvia a pieno schermo con xinerama (mostra su "
"diversi schermi),\n"

#: src/xitk/main.c:496
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               nascondi GUI (pannello, etc.)\n"

#: src/xitk/main.c:497
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             nascondi finestra video\n"

#: src/xitk/main.c:499
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Disattiva il supporto di LIRC.\n"

#: src/xitk/main.c:501
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
msgstr ""
"      --visual <classe-o-id>   Usa la specificata <classe-o-id> di "
"visualizzazione X11\n"

#: src/xitk/main.c:502
msgid ""
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"                               o è un classe di visualizzazione X11, o è un "
"id visuale.\n"

#: src/xitk/main.c:503
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Installa una mappa colore privata.\n"

#: src/xitk/main.c:504
msgid "      --keymap [=option]       Display keymap. Option are:\n"
msgstr ""
"      --keymap [=opzione]       Mostra una mappa tasti. Le opzioni sono:\n"

#: src/xitk/main.c:505
msgid ""
"                                 'default': display default keymap table,\n"
msgstr ""
"                                 'default': mostra la tabella predefinita "
"della mappa di tasti,\n"

#: src/xitk/main.c:506
msgid ""
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
msgstr ""
"                                 'lirc': mostra bozza di un file di "
"configurazione .lircrc.\n"

#: src/xitk/main.c:507
msgid ""
"                                 'remapped': user remapped keymap table.\n"
msgstr ""
"                                 'remapped': tabella di mappa tasti "
"rimappata dell'utente.\n"

#: src/xitk/main.c:508
msgid ""
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
msgstr ""
"                                 'file:<nome file>': usa <nome file> quale "
"mappa tasti.\n"

#: src/xitk/main.c:509
msgid ""
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"                                 -se non si dà alcun opzione, si seleziona "
"'default'.\n"

#: src/xitk/main.c:510
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Abilita il server di controllo remoto della "
"rete.\n"

#: src/xitk/main.c:511
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Usa la finestra radice come finestra video.\n"

#: src/xitk/main.c:512
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <WxH[+X+Y]>   Imposta la geometria della finestra di output "
"(stile X).\n"

#: src/xitk/main.c:513
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Finestra di output video senza bordo.\n"

#: src/xitk/main.c:514
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
msgstr ""
"  -N, --animation <MRL>        Specifica l'MRL da riprodurre quando l'output "
"video non è utilizzato.\n"

#: src/xitk/main.c:515
msgid "                                 -can be used more than one time.\n"
msgstr "                                 -può essere usato più di una volta.\n"

#: src/xitk/main.c:516
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr ""
"  -P, --playlist <nome file>    Carica un file contenente la playlist.\n"

#: src/xitk/main.c:517
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
msgstr ""
"  -l, --loop [=mode]           Imposta il modo di ripetizione della playlist "
"(predefinito: ciclico). I modi sono:\n"

#: src/xitk/main.c:518
msgid "                                 'loop': loop entire playlist.\n"
msgstr ""
"                                 'loop': ripetizione ciclica dell'intera "
"playlist.\n"

#: src/xitk/main.c:519
msgid ""
"                                 'repeat': repeat current playlist entry.\n"
msgstr ""
"                                 'repeat': ripete il corrente elemento della "
"playlist.\n"

#: src/xitk/main.c:520
msgid ""
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
msgstr ""
"                                 'shuffle': seleziona casualmente un "
"elemento non ancora riprodotto dalla playlist\n"

#: src/xitk/main.c:521
msgid ""
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"                                 'shuffle+': stessa cosa di 'shuffle', con "
"la differenza che la playlist viene rieseguita indefinitamente.\n"

