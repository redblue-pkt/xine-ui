msgid "                    volume=v    set audio volume,\n"
msgstr "                    volume=v    imposta volume audio,\n"

#: src/xitk/main.c:503
msgid "                    amp=v       set amplification level,\n"
msgstr "                    amp=v       imposta livello di amplificazione,\n"

#: src/xitk/main.c:504
msgid "                    (playlist|pl)=p\n"
msgstr "                    (playlist|pl)=p\n"

#: src/xitk/main.c:505
msgid "                                 <p> can be:\n"
msgstr "                                 <p> può essere:\n"

#: src/xitk/main.c:506
msgid "                                   'clear'  clear the playlist,\n"
msgstr "                                   'clear'  cancella la playlist,\n"

#: src/xitk/main.c:507
msgid "                                   'next'   play next playlist entry,\n"
msgstr ""
"                                   'next'   riproduce il prossimo elemento "
"della playlist,\n"

#: src/xitk/main.c:508
msgid ""
"                                   'prev'   play previous playlist entry,\n"
msgstr ""
"                                   'prev'   riproduce il precedente elemento "
"della playlist,\n"

