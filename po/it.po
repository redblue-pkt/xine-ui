msgstr "                    session=n   specifica sessione numero <n>,\n"

#: src/xitk/main.c:499
msgid "                    mrl=m       add mrl <m> to the playlist,\n"
msgstr "                    mrl=m       aggiunge MRL <m> alla playlist,\n"

#: src/xitk/main.c:500
msgid ""
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
msgstr ""
"                    audio=c     seleziona canale audio (<c>: 'next' o "
"'prev'),\n"

#: src/xitk/main.c:501
msgid ""
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
msgstr ""
"                    spu=c       seleziona canale SPU (<c>: 'next' o "
"'prev'),\n"

