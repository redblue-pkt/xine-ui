msgstr "                                   'clear'  cancella la playlist,\n"

#: src/xitk/main.c:535
msgid "                                   'next'   play next playlist entry,\n"
msgstr ""
"                                   'next'   riproduce il prossimo elemento "
"della playlist,\n"

#: src/xitk/main.c:536
msgid ""
"                                   'prev'   play previous playlist entry,\n"
msgstr ""
"                                   'prev'   riproduce il precedente elemento "
"della playlist,\n"

#: src/xitk/main.c:537
msgid "                                   'load:s' load playlist file <s>.\n"
msgstr ""
"                                   'load:s' carica la playlilst dal file "
"<s>.\n"

#: src/xitk/main.c:538
msgid "                    play, slow2, slow4, pause, fast2,\n"
msgstr "                    play, slow2, slow4, pause, fast2,\n"

#: src/xitk/main.c:539
msgid "                    fast4, stop, quit, fullscreen, eject.\n"
msgstr "                    fast4, stop, quit, fullscreen, eject.\n"

