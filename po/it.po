msgid ""
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
msgstr ""
"                    audio=c     seleziona canale audio (<c>: 'next' o "
"'prev'),\n"

#: src/xitk/main.c:529
msgid ""
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
msgstr ""
"                    spu=c       seleziona canale SPU (<c>: 'next' o "
"'prev'),\n"

