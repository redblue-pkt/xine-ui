#: src/xitk/main.c:579
#, c-format
msgid "OPTIONS are:\n"
msgstr "Le OPZIONI sono:\n"

#: src/xitk/main.c:580
#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Mostra versione.\n"

#: src/xitk/main.c:581
#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=livello]       Imposta il livello di verbosity. Il valore "
"predefinito è 1.\n"

#: src/xitk/main.c:582
#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <file>          Usa il file di configurazione indicato invece "
"di quello predefinito.\n"

#: src/xitk/main.c:583
#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Seleziona il driver video driver per id. "
"Driver disponibili: \n"

#: src/xitk/main.c:594
#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Seleziona il driver audio per id. Driver "
"disponibili: \n"

#: src/xitk/main.c:604
#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Seleziona il canale SPU (sottotitolo) '#'.\n"

#: src/xitk/main.c:605
#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Seleziona il canale audio '#'.\n"

#: src/xitk/main.c:606
#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Esegue all'avvio. Può essere seguito da:\n"
"                    'f': in modalità a schermo pieno.\n"
"                    'h': nasconde la GUI (pannello, ecc...).\n"
"                    'w': masconde la finestra video.\n"
"                    'q': termina quando è finita la riproduzione.\n"
"                    'd': ricava la playlist dal DVD. (deprecato. Usa -s "
"DVD)\n"
"                    'v': ricava la playlist dal VCD. (deprecato. Usa -s "
"VCD)\n"

#: src/xitk/main.c:614
#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': modalità a pieno schermo con xinerama.\n"

#: src/xitk/main.c:616
#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     scansione automatica playlist dal <plugin>\n"

#: src/xitk/main.c:617
#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             avvia in modalità a schermo pieno,\n"

#: src/xitk/main.c:619
#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           avvia a pieno schermo con xinerama (mostra su "
"diversi schermi),\n"

#: src/xitk/main.c:621
#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               nascondi GUI (pannello, etc.)\n"

#: src/xitk/main.c:622
#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "  -g, --hide-gui               disabilita GUI (pannello, etc.)\n"

#: src/xitk/main.c:623
#, c-format
msgid "      --no-mouse               disable mouse event\n"
msgstr "      --no-mouse               disattiva gli eventi del mouse\n"

#: src/xitk/main.c:624
#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             nascondi finestra video\n"

#: src/xitk/main.c:626
#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Disattiva il supporto di LIRC.\n"

#: src/xitk/main.c:628
#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <class-or-id>   Usa la visuale X11 specificata. <class-or-"
"id>\n"
"                               è o una classe visuale di X11, o un id "
"visuale.\n"

#: src/xitk/main.c:630
#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Installa una mappa colore privata.\n"

#: src/xitk/main.c:631
#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=opzioni]       Mostra la mappa dei tasti. Le opzioni sono:\n"
"                                 'default': mostra la tabella della mappa "
"dei tasti predefinita,\n"
"                                 'lirc': mostra un abozzo del file di "
"configurazione .lircrc.\n"
"                                 'remapped': tabella della mappa dei tasti "
"rimappati dall'utente.\n"
"                                 'file:<filename>': usa <filename> come "
"mappa dei tasti.\n"
"                                 -se non si specificano opzioni, si "
"seleziona 'default'.\n"

#: src/xitk/main.c:637
#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Abilita il server di controllo remoto della "
"rete.\n"

#: src/xitk/main.c:638
#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr ""
"      --network-port           Specifica la porta del server di rete.\n"

#: src/xitk/main.c:639
#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Usa la finestra radice come finestra video.\n"

#: src/xitk/main.c:640
#, c-format
msgid "  -W, --wid                    Use a window id.\n"
msgstr "  -W, --wid                    Utilizza un certo id finestra.\n"

#: src/xitk/main.c:641
#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <WxH[+X+Y]>   Imposta la geometria della finestra di output "
"(stile X).\n"

#: src/xitk/main.c:642
#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Finestra di output video senza bordo.\n"

#: src/xitk/main.c:643
#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        Specifica l'MRL da riprodurre quando l'output "
"video non è utilizzato.\n"
"                                 -può essere usata più di una volta.\n"

#: src/xitk/main.c:645
#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr ""
"  -P, --playlist <nome file>    Carica un file contenente la playlist.\n"

#: src/xitk/main.c:646
#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=modalità]           Imposta la modalità di ciclo della "
"playlist (predefinita: loop). Le modalità sono:\n"
"                                 'loop': fa' una ripetizione ciclica "
"dell'intera playlist.\n"
"                                 'repeat': ripete la voce della playlist "
"corrente.\n"
"                                 'shuffle': seleziona casualmente una voce "
"non ancora eseguita dalla playlist\n"
"                                 'shuffle+': come 'shuffle', ma riesegue "
"indefinitamente la playlist.\n"

#: src/xitk/main.c:651
#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr ""
"      --skin-server-url <URL>  Definisce la URL del server delle skin.\n"

#: src/xitk/main.c:652
#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <MRL>          Accoda l'MRL di una sessione in esecuzione "
"(sessione 0)\n"

#: src/xitk/main.c:653
#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <opzione1,opzione2,...>\n"
"                               Gestori di sessione. Le opzioni possono "
"essere:\n"
"                    session=n   specifica la sessione numero <n>,\n"
"                    mrl=m       aggiunge l'MRL <m> alla playlist,\n"
"                    audio=c     seleziona il canale audio (<c>: "
"'next' (prossimo) o 'prev' (precedente) ),\n"
"                    spu=c       seleziona il canale spu (<c>: "
"'next' (prossimo) or 'prev' (precedente) ),\n"
"                    volume=v    imposta il volume audio,\n"
"                    amp=v       imposta il livello di amplificazione,\n"
"                    loop=m      imposta le modalità di ciclo (<m>: 'none' "
"'loop' 'repeat' 'shuffle' or 'shuffle+'),\n"
"                    get_speed   recupera il valore di velocita (si veda la "
"man page per il valore ritornato).\n"
"                    (playlist|pl)=p\n"
"                                 <p> può essere:\n"
"                                   'clear'  ripulisce la playlist,\n"
"                                   'first'  esegue la prima voce nella "
"playlist,\n"
"                                   'prev'   esegue la precedente voce nella "
"playlist,\n"
"                                   'next'   esegue la prossima voce nella "
"playlist,\n"
"                                   'last'   esegue l'ultima voce nella "
"playlist,\n"
"                                   'load:s' carica il file <s> della "
"playlist,\n"
"                                   'stop'   termina la riproduzione alla "
"fine della corrente.\n"
"                                   'cont'   continua la riproduzione alla "
"fine della corrente.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#: src/xitk/main.c:675
#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           Non riproduce automaticamente (modo "
"intelligente).\n"

#: src/xitk/main.c:676
#, c-format
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""
"  -D, --deinterlace [post]...  Deinterlaccia output video. Si possono "
"specificare uno \n"
"                                 o più post plugin, con parametri "
"opzionali.\n"
"                                 La sintassi è la stessa dell'opzione --"
"post.\n"

#: src/xitk/main.c:679
#, c-format
msgid ""
"  -r, --aspect-ratio <mode>    Set aspect ratio of video output. Modes are:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"
msgstr ""
"  -r, --aspect-ratio <mode>    Imposta le proporzioni dell'output video. Le "
"modalità sono:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"

#: src/xitk/main.c:682
#, c-format
msgid ""
"      --broadcast-port <port>  Set port of xine broadcaster (master side)\n"
"                               Slave is started with 'xine slave://address:"
"port'\n"
msgstr ""
"      --broadcast-port <port>  Imposta la porta del broadcast di xine (lato "
"master)\n"
"                               Lo slave si avvia con 'xine slave://address:"
"port'\n"

#: src/xitk/main.c:685
#, c-format
msgid "      --no-logo                Don't display the logo.\n"
msgstr "      --no-logo                Non mostra il logo.\n"

#: src/xitk/main.c:686
#, c-format
msgid "  -E, --no-reload              Don't reload old playlist.\n"
msgstr "  -E, --no-reload              Non ricarica la vecchia playlist.\n"

#: src/xitk/main.c:687
#, c-format
msgid ""
"      --post <plugin>[:parameter=value][,...][;...]\n"
"                               Load one or many post plugin(s).\n"
"                               Parameters are comma separated.\n"
"                               This option can be used more than one time.\n"
msgstr ""
"      --post <plugin>[:parametro=valore][,...][;...]\n"
"                               Carica uno o più post plugin.\n"
