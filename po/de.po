#: src/xitk/main.c:579
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Benutzung: xine [OPTIONEN]... [MRL]\n"

#: src/xitk/main.c:582
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Zeigt die Version an.\n"

#: src/xitk/main.c:583
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=Stufe]       Geschwätzigkeit. Wenn keine Stufe angegeben: "
"1.\n"

#: src/xitk/main.c:584
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <Datei>          Angegebene Konfigurationsdatei benutzen.\n"

#: src/xitk/main.c:585
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Auswahl des Video Treiber nach id. "
"Verfügbar: \n"

#: src/xitk/main.c:596
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <dvr>     Auswahl des Audio Treibers nach id. "
"Verfügbar: \n"

#: src/xitk/main.c:606
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Auswahl des SPU (Untertitel) Kanals '#'.\n"

#: src/xitk/main.c:607
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Auswahl des Audio Kanals '#'.\n"

#: src/xitk/main.c:608
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Beim starten abspielen. Optional gefolgt "
"von:\n"
"                    'f': im Vollbildmodus.\n"
"                    'h': GUI Verstecken (Bedienfeld, usw.).\n"
"                    'w': Video-Fenster verstecken.\n"
"                    'q': Nach dem abspielen beenden.\n"
"                    'd': DVD-Playlist auslesen. (Veraltet. Nutzen Sie -s "
"DVD)\n"
"                    'v': VCD-Playliste auslesen. (Veraltet. Nutzen Sie -s "
"VCD)\n"

#: src/xitk/main.c:616
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': im Xinerama Vollbildmodus.\n"

#: src/xitk/main.c:618
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     Playliste automatisch vom <plugin> auslesen\n"

#: src/xitk/main.c:619
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             im Vollbildmodus starten,\n"

#: src/xitk/main.c:621
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -f, --fullscreen             im Xinerama Vollbildmodus starten (Anzeige "
"über mehrere Bildschirme),\n"

#: src/xitk/main.c:623
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               GUI verstecken (Bedienfeld, usw.)\n"

#: src/xitk/main.c:624
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             Video-Fenster verstecken\n"

#: src/xitk/main.c:626
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                LIRC-Unterstützung abschalten.\n"

#: src/xitk/main.c:628
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <Klasse-oder-id> Angegebenes X11 Visual benutzen\n"
"                                <Klasse-oder-id> ist entweder eine X11\n"
"                                visual Klasse oder eine visual id.\n"

#: src/xitk/main.c:630
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Installiert eine eigene Farbtabelle.\n"

#: src/xitk/main.c:631
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=Option]       Zeigt Tastentabelle. Optionen sind:\n"
"                                 'default': Zeige Standard Tastentabelle,\n"
"                                 'lirc': Zeigt Beispiel einer .lircrc-"
"Datei.\n"
"                                 'remapped': Zeige aktuelle Tastentabelle.\n"
"                                 'file:<Dateiname>': benutze <Dateiname>\n"
"                                                     als Tastentabelle.\n"
"                                  -Ohne Angabe wird 'default' benutzt.\n"

#: src/xitk/main.c:637
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Aktiviere Fernsteuerung über Netzwerk.\n"

#: src/xitk/main.c:638
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Desktophintergrund für Video benutzen.\n"

#: src/xitk/main.c:639
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <BxH[+X+Y]>    setze Fenster-Geometrie (X-typisches "
"Format)\n"

#: src/xitk/main.c:640
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless              Rahmenloses Video-Fenster.\n"

#: src/xitk/main.c:641
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        MRL zum Abspielen, falls keine Videoanteil "
"vorhanden ist.\n"
"                                 -Kann mehr als einmal angegeben werden.\n"

#: src/xitk/main.c:643
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr "  -P, --playlist <Dateiname>   Lädt eine Playliste.\n"

#: src/xitk/main.c:644
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=Modus]          Setzt Wiederholmodus der Playliste. Modi "
"sind:\n"
"                                'loop': Gesamte Playlist wiederholen. "
"(Standard)\n"
"                                'repeat': Aktuellen Playlisteintrag "
"wiederholen.\n"
"                                'shuffle': Zufälligen ungespielten Eintrag "
"aus                                           Playlist auswählen.\n"
"                                'shuffle+': Ähnlich wie 'shuffle', nur wird "
"die                                            Playliste unendlichoft "
"wiederholt.\n"

#: src/xitk/main.c:649
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  URL für Skin-Server.\n"

#: src/xitk/main.c:650
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <mrl> ...      MRL in laufende Sitzung (Sitzung 0) anhängen\n"

#: src/xitk/main.c:651
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <Option1,Option2,...>\n"
"                               Sitzungsverwaltung. Option kann sein:\n"
"                    session=n   Sitzungsnummer <n> angeben,\n"
"                    mrl=m       MRL <m> der Playliste hinzufügen,\n"
"                    audio=c     Audiokanal wählen (<c>: 'next' oder "
"'prev'),\n"
"                    spu=c       SPU-Kanal wählen (<c>: 'next' oder 'prev'),\n"
"                    volume=v    Audiolautstärke <v> setzen,\n"
"                    amp=v       Audioverstärkungslevel <v> setzen,\n"
"                    loop=m      Wiederholmodus (<m>: 'none', 'loop', "
"'repeat',                                                'shuffle' oder "
"'shuffle+'),\n"
"                    (playlist|pl)=p\n"
"                      <p> kann sein:\n"
"                        'clear'  Playliste löschen,\n"
"                        'first'  Ersten Eintrag in Playliste wiedergeben,\n"
"                        'prev'   Vorherigen Eintrag in Playliste "
"wiedergeben,\n"
"                        'next'   Nächsten Eintrag in Playliste wiedergeben,\n"
"                        'last'   Letzten Eintrag in Playliste wiedergeben,\n"
"                        'load:s' Playliste aus Datei <s> laden,\n"
"                        'stop'   Wiedergabe nach momentanen Titel stoppen.\n"
"                        'cont'   Wiedergabe nach momentaten Titel "
"weiterführen.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#: src/xitk/main.c:672
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           Wiedergabe nicht automatisch starten (Smart-"
"Modus).\n"

#: src/xitk/main.c:673
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""
"  -D, --deinterlace [post]...  Deinterlace Video-Ausgabe. Ein oder mehrere "
"Nach-Plugins\n"
"                                 können mit optionalen Parametern angegeben "
"werden.\n"
"                                 Syntax ist die Selbe wie bei der --post-"
"Option.\n"

#: src/xitk/main.c:676
msgid ""
"  -r, --aspect-ratio <mode>    Set aspect ratio of video output. Modes are:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"
msgstr ""
"  -r, --aspect-ratio <modus>   Setzt Seitenverhältnis für Video-Ausgabe. "
"Modi sind:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"

#: src/xitk/main.c:679
msgid ""
"      --broadcast-port <port>  Set port of xine broadcaster (master side)\n"
"                               Slave is started with 'xine slave://address:"
