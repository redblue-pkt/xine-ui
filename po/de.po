msgstr "                                   'clear'  Playliste löschen,\n"

#: src/xitk/main.c:504
msgid "                                   'next'   play next playlist entry,\n"
msgstr ""
"                                   'next'   Nächsten Eintrag in Playliste "
"wiedergeben,\n"

#: src/xitk/main.c:505
msgid ""
"                                   'prev'   play previous playlist entry,\n"
msgstr ""
"                                   'prev'   Vorherigen Eintrag in Playliste "
"wiedergeben,\n"

#: src/xitk/main.c:506
msgid "                                   'load:s' load playlist file <s>.\n"
msgstr ""
"                                   'load:s' Playliste aus Datei <s> laden.\n"

#: src/xitk/main.c:507
msgid "                    play, slow2, slow4, pause, fast2,\n"
msgstr "                    play, slow2, slow4, pause, fast2,\n"

#: src/xitk/main.c:508
