#: src/xitk/main.c:577
#, c-format
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Použití: xine [VOLBY]... [MRL]\n"

#: src/xitk/main.c:579
#, c-format
msgid "OPTIONS are:\n"
msgstr "VOLBY jsou:\n"

#: src/xitk/main.c:580
#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Zobrazení verze.\n"

#: src/xitk/main.c:581
#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=úroveň]      Nastavit úroveň množství výpisů. Implicitní "
"je 1.\n"

#: src/xitk/main.c:582
#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <soubor>        Použít konfigurační soubor místo "
"standardního.\n"

#: src/xitk/main.c:583
#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Výběr video ovladače. K dispozici jsou: \n"

#: src/xitk/main.c:594
#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Výběr zvukového ovladače. K dispozici jsou: \n"

#: src/xitk/main.c:604
#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr "  -u, --spu-channel <#>        Výběr kanálu SPU (titulky).\n"

#: src/xitk/main.c:605
#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Výběr zvukového kanálu '#'.\n"

#: src/xitk/main.c:606
#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Při startu začít přehrávat. Může následovat:\n"
"                    'f': v celoobrazovkovém režimu.\n"
"                    'h': skrýt GUI (panel, atd.).\n"
"                    'w': skrýt okno s videem.\n"
"                    'q': skončit, když skončí přehrávání.\n"
"                    'd': získat playlist z DVD. (raději použijte -s DVD)\n"
"                    'v': získat playlist z VCD. (raději použijte -s VCD)\n"

#: src/xitk/main.c:614
#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': v celoobrazovkovém režimu xinerama.\n"

#: src/xitk/main.c:616
#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr "  -s, --auto-scan <modul>      Získat playlist z daného modulu\n"

#: src/xitk/main.c:617
#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             Začít v celoobrazovkovém režimu\n"

#: src/xitk/main.c:619
#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           Začít v celoobrazovkovém režimu xinerama "
"(zobrazení na několika obrazovkách),\n"

#: src/xitk/main.c:621
#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               Skrýt GUI (panel, atd.)\n"

#: src/xitk/main.c:622
#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "  -I, --no-gui                 Zakázat GUI.\n"

#: src/xitk/main.c:623
#, c-format
msgid "      --no-mouse               disable mouse event\n"
msgstr "      --no-mouse               Zakázat události myši.\n"

#: src/xitk/main.c:624
#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             Skrýt okno s videem.\n"

#: src/xitk/main.c:626
#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                Vypnout podporu LIRC.\n"

#: src/xitk/main.c:628
#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <třída-nebo-id> Použít uvedené zobrazení X11.\n"
"                               Jde buď o třídu zobrazení X11 nebo id. "
"obrazovky.\n"

#: src/xitk/main.c:630
#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Instalovat vlastní paletu.\n"

#: src/xitk/main.c:631
#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=volba]        Zobrazit mapu klávesnice. Volby jsou:\n"
"                                 'default': zobrazit implicitní mapu "
"klávesnice,\n"
"                                 'lirc': zobrazit vzorový konfigurační "
"soubor .lircrc.\n"
"                                 'remapped': uživatelem pozměněnou mapu "
"klávesnice.\n"
"                                 'file:<soubor>': použít <soubor> jako mapu "
"klávesnice.\n"
"                                 -jestliže není zadána žádná volba, je "
"vybráno 'default'.\n"

#: src/xitk/main.c:637
#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Povolit vzdálenou síťovou kontrolu serveru.\n"

#: src/xitk/main.c:638
#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr "      --network-port           Uvézt číslo portu síťového serveru.\n"

#: src/xitk/main.c:639
#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Pro zobrazení videa použít pracovní plochu.\n"

#: src/xitk/main.c:640
#, c-format
msgid "  -W, --wid                    Use a window id.\n"
msgstr "  -W, --wid                    Použít id okna.\n"

#: src/xitk/main.c:641
#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometry <WxH[+X+Y]>   Nastavit velikost a umístění výstupního okna "
"(ve stylu X).\n"

#: src/xitk/main.c:642
#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -B, --borderless             Výstupní okno s videem bez okrajů\n"

#: src/xitk/main.c:643
#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <MRL>        Specifikování MRL, které se má přehrát, když "
"není použit výstup videa.\n"
"                                 -může být použito více než jednou.\n"

#: src/xitk/main.c:645
#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr "  -P, --playlist <soubor>      Načíst soubor playlistu.\n"

#: src/xitk/main.c:646
#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=režim]          Nastavit režim opakování playlistu "
"(implicitně: loop). Režimy jsou:\n"
"                                 'loop': opakovat celý playlist.\n"
"                                 'repeat': opakovat aktuální položku "
"playlistu.\n"
"                                 'shuffle': náhodně vybrat zatím nepřehranou "
"položku playlistu.\n"
"                                 'shuffle+': stejně jako 'shuffle', ale do "
"nekonečna přehrávat playlist.\n"

#: src/xitk/main.c:651
#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <URL>  Definovat URL serveru se skiny.\n"

#: src/xitk/main.c:652
#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"      --enqueue <MRL> ...      Zařadit MRL do fronty běžící relace (relace "
"0)\n"

#: src/xitk/main.c:653
#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <volba1,volba2,...>\n"
"                               Správa relací. Volby mohou být:\n"
"                    session=n   specifikování čísla relace <n>,\n"
"                    mrl=m       přidat MRL <m> do playlistu,\n"
"                    audio=k     vybrat zvukový kanál (<k>: 'next' nebo "
"'prev'),\n"
"                    spu=k       vybrat kanál s titulky (<k>: 'next' nebo "
"'prev'),\n"
"                    volume=v    nastavit hlasitost zvuku,\n"
"                    amp=v       nastavit úroveň zesílení\n"
"                    loop=r      nastavit režim opakování (<r>: 'none' 'loop' "
"'repeat' 'shuffle' nebo 'shuffle+'),\n"
"                    get_speed   získat hodnotu rychlosti (hodnoty viz. "
"manuálová stránka).\n"
"                    (playlist|pl)=p\n"
