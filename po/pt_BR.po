#: src/xitk/main.c:269
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Uso: xine [OPÇÕES]... [MRL]\n"

#: src/xitk/main.c:272
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Mostrar versão.\n"

#: src/xitk/main.c:273
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Selecionar um driver de vídeo pelo "
"identificador. Drivers disponíveis: \n"

#: src/xitk/main.c:282
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Selecionar um driver de audio pelo "
"identificador. Drivers disponíveis: \n"

#: src/xitk/main.c:292
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Selecionar o canal de SPU (legenda) '#'.\n"

#: src/xitk/main.c:293
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Selecionar o canal de audio '#'.\n"

#: src/xitk/main.c:294
msgid "  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
msgstr ""
"  -p, --auto-play [opt]        Iniciar tocando (play). Pode ser especificado "
"também:\n"

#: src/xitk/main.c:295
msgid "                    'f': in fullscreen mode.\n"
msgstr "                    'f': modo tela cheia.\n"

#: src/xitk/main.c:296
msgid "                    'h': hide GUI (panel, etc.).\n"
msgstr ""
"                    'h': esconder a interface gráfica (painel, etc.).\n"

#: src/xitk/main.c:297
msgid "                    'w': hide video window.\n"
msgstr "                    'w': esconder a janela de vídeo\n"

#: src/xitk/main.c:298
msgid "                    'q': quit when play is done.\n"
msgstr "                    'q': sai do programa quando terminar de tocar.\n"

#: src/xitk/main.c:299
msgid ""
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
msgstr ""
"                    'd': pegar a \"playlist\" do DVD. (obsoleto, usar -s "
"DVD)\n"

#: src/xitk/main.c:300
msgid ""
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"                    'v': pegar a \"playlist\" do VCD. (obsoleto, usar -s "
"VCD)\n"

#: src/xitk/main.c:301
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     procurar automaticamente a playlist do "
"<plugin>\n"

#: src/xitk/main.c:302
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             iniciar em modo de tela cheia,\n"

#: src/xitk/main.c:303
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr ""
"  -g, --hide-gui               esconder a interface gráfica (painel, etc.)\n"

#: src/xitk/main.c:304
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             esconder a janela de vídeo\n"

#: src/xitk/main.c:306
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr ""
"  -L, --no-lirc                Desligar suporte a LIRC (controle remoto).\n"

#: src/xitk/main.c:308
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
msgstr ""
"      --visual <class-or-id>   Usar o visual X11 especificado. <class-or-"
"id>\n"

#: src/xitk/main.c:309
