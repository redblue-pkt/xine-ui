#: src/xitk/main.c:138
#, c-format
msgid "Built with xine library %d.%d.%d [%s]-[%s]-[%s].\n"
msgstr "Compilado com xine library %d.%d.%d [%s]-[%s]-[%s].\n"

#: src/xitk/main.c:141
#, c-format
msgid "Found xine library version: %d.%d.%d (%s).\n"
msgstr "Encontrada xine library versão: %d.%d.%d (%s).\n"

#: src/xitk/main.c:156
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Utilização: xine [OPÇÕES]... [MRL]\n"

#: src/xitk/main.c:159
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Mostra versão.\n"

#: src/xitk/main.c:160
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Seleciona um driver de vídeo pelo "
"identificador. Drivers disponíveis: \n"

#: src/xitk/main.c:169
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Seleciona um driver de audio pelo "
"identificador. Drivers disponíveis: \n"

#: src/xitk/main.c:179
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr ""
"  -u, --spu-channel <#>        Seleciona o canal de SPU (legenda) '#'.\n"

#: src/xitk/main.c:180
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Seleciona o canal de audio '#'.\n"

#: src/xitk/main.c:181
msgid "  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
msgstr ""
"  -p, --auto-play [opt]        Inicia com play. Pode ser especificado "
"também:\n"

#: src/xitk/main.c:182
msgid "                    'f': in fullscreen mode.\n"
msgstr "                    'f': modo tela cheia.\n"

#: src/xitk/main.c:183
msgid "                    'h': hide GUI (panel, etc.).\n"
msgstr ""
"                    'h': não mostra a interface gráfica (painel, etc.).\n"

#: src/xitk/main.c:184
#, fuzzy
msgid "                    'w': hide video window.\n"
msgstr "  -H, --hide-video             Não mostra a janela de vídeo\n"

#: src/xitk/main.c:185
msgid "                    'q': quit when play is done.\n"
msgstr "                    'q': sai do programa quando terminar de tocar.\n"

#: src/xitk/main.c:186
msgid ""
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
msgstr ""
"                    'd': pega playlist do DVD. (obsoleto, usar -s DVD)\n"

#: src/xitk/main.c:187
msgid ""
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"                    'v': pega playlist do VCD. (obsoleto, usar -s VCD)\n"

#: src/xitk/main.c:188
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr "  -s, --auto-scan <plugin>     Pega playlist do <plugin>\n"

#: src/xitk/main.c:189
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             inicia em modo de tela cheia,\n"

#: src/xitk/main.c:190
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr ""
"  -g, --hide-gui               não mostra a interface gráfica (painel, "
"etc.)\n"

#: src/xitk/main.c:191
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             Não mostra a janela de vídeo\n"

#: src/xitk/main.c:193
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr ""
"  -L, --no-lirc                Desliga suporte a LIRC (controle remoto).\n"

#: src/xitk/main.c:195
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
msgstr ""
"      --visual <class-or-id>   Especifica X11 visual que deve ser usado. "
"<class-or-id>\n"

#: src/xitk/main.c:196
msgid ""
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"                               pode ser tanto um \"X11 visual class\", or um "
"\"visual id\".\n"

#: src/xitk/main.c:197
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Instala uma colormap privada.\n"

#: src/xitk/main.c:198
msgid "      --keymap [option]        Display keymap. Option are:\n"
msgstr "      --keymap [option]        Mostra mapa de teclas. Opções são:\n"

#: src/xitk/main.c:199
msgid ""
"                                 'default': display default keymap table,\n"
msgstr "                                 'default': tabela padrão de teclas,\n"

#: src/xitk/main.c:200
msgid ""
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
msgstr ""
"                                 'lirc': rascunho de uma configuração para o "
"arquivo lircrc.\n"

#: src/xitk/main.c:201
msgid ""
"                                 'remapped': user remapped keymap table.\n"
msgstr ""
"                                 'remapped': tabela remapeada pelo usuário.\n"

#: src/xitk/main.c:202
msgid ""
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"                                 -se nenhum opção for dada, 'default' é "
"assumido.\n"

#: src/xitk/main.c:204
msgid "examples for valid MRLs (media resource locator):\n"
msgstr "examplos de MRLs (media resource locator) validos:\n"

#: src/xitk/main.c:205
