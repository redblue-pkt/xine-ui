msgstr "                    session=n   spécifie le numéro de session <n>,\n"

#: src/xitk/main.c:527
msgid "                    mrl=m       add mrl <m> to the playlist,\n"
msgstr ""
"                    mrl=m       ajoute la mrl <m> à la liste de lecture,\n"
"\n"

