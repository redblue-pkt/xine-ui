msgstr ""
"                                   'clear'  efface la liste de lecture,\n"

#: src/xitk/main.c:535
msgid "                                   'next'   play next playlist entry,\n"
msgstr "                                   'next'   joue l'entrée suivante,\n"

#: src/xitk/main.c:536
msgid ""
"                                   'prev'   play previous playlist entry,\n"
msgstr ""
"                                   'prev'   joue l'entrée précédente,\n"

