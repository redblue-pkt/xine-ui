#, c-format
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Użytkowanie: xine [OPCJE]... [MRL]\n"

#, c-format
msgid "OPTIONS are:\n"
msgstr "OPCJE są:\n"

#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Wyświetlenie numeru wersji.\n"

#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=poziom]       Ustaw poziom \"gadatliwości\" programu. "
"Domyślnie 1.\n"

#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <plik>          Użyj podanego pliku konfiguracyjnego zamiast "
"domyślnego.\n"

#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Wybór sterownika wideo. Dostępne "
"sterowniki: \n"

#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Wybór sterownika dźwięku. Dostępne "
"sterowniki: \n"

#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr "  -u, --spu-channel <#>        Wybór kanału SPU (napisy) '#'.\n"

#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "  -a, --audio-channel <#>      Wybór kanału dźwięku '#'.\n"

#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Rozpocznij odtwarzanie. Dostępne opcje:\n"
"                    'f': tryb pełnoekranowy.\n"
"                    'h': ukryj interfejs użytkownika (panel, itp.).\n"
"                    'w': ukryj okno obrazu.\n"
"                    'q': zakończ wraz z końcem odtwarzania.\n"
"                    'd': szukaj listy odtwarzania na DVD. (przestarzałe. "
"używaj -s DVD)\n"
"                    'v': szukaj listy odtwarzania na VCD. (przestarzałe. "
"używaj -s VCD)\n"

#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': tryb pełnoekranowy w trybie xinerama\n"

#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr "  -s, --auto-scan <wtyczka>    auto-skanowanie listy z <wtyczki>\n"

#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --fullscreen             rozpoczęcie w trybie pełnoekranowym,\n"

#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           rozpoczęcie w trybie pełnoekranowym, xinerama "
"-  (na kilku ekranach),\n"

#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               ukrycie GUI (panel, etc.)\n"

#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "  -I, --no-gui               wyłącz GUI\n"

#, c-format
msgid "      --no-mouse               disable mouse event\n"
msgstr ""

#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             ukrycie okna wideo\n"

#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, --no-lirc                wyłączenie obsługi dla LIRC'a\n"

#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <class-or-id>   Użyj podanego wyświetlania X11. <class-or-"
"id>\n"
"                               jest albo klasą ekranu X11, albo id ekranu.\n"

#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --install                Instalowanie prywatnej mapy kolorów.\n"

#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=opcja]       Wyświetl mapowanie klawiszy. Opcjami są:\n"
"                                 'default': pokaż domyślne mapowanie "
"klawiszy,\n"
"                                 'lirc': wyświetl szablon pliku "
"konfiguracyjnego .lircrc\n"
"                                  'remapped': zmodyfikowane mapowanie "
"użytkownika.\n"
"                                 'file:<plik>': użyj <plik> jako mapowania "
"klawiszy\n"
"                                 -jeśli brak opcji - użyty jest 'default' "
"domyślnie.\n"

#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Włącz sieciowy serwer zdalnej kontroli.\n"

#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr "      --network-port           Podaj numer portu serwera sieciowego.\n"

#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr "  -R, --root                   Użyj okna głównego jako okna wideo\n"

#, c-format
msgid "  -W, --wid                    Use a window id.\n"
msgstr "  -W, --wid                    Użyj id okna wideo\n"

#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr "  -G, --geometry <WxH[+X+Y]>   Ustawianie geometrii okna (X style).\n"

#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr "  -H, --hide-video             ukrywanie okna wideo\n"

#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, -animation <mrl>        Podaje MRL do odegrania w przypadku gdy "
"strumień nie posiada obrazu.\n"
"                                 -może być używane więcej niż raz.\n"

#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr "  -P, --palylist <filname>     Ładuj plik z listą odtwarzania.\n"

#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=mode]           Ustaw tryb powtarzania listy odtwarzania "
"(domyślnie: loop). Tryby:\n"
"                                 'loop': powtarza całą listę odtwarzania.\n"
"                                 'repeat': powtarza bierzącą pozycję z "
"listy.\n"
"                                 'shuffle': wybiera z listy losowo "
"nieodtwarzane jeszcze pozycje\n"
"                                 'shuffle+': to samo co 'shuffle' ale lista "
"powtarzana jest bez końca.\n"

#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  Definiuje adres serwera skór.\n"

#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"       --enqueue <mrl> ...      Kolejkuj MRL w bierzącej sesji (sesja 0)\n"

#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"  -S, --session <opcja1,opcja2,...>\n"
"                               Zarządzanie sesjami. Dostępne opcje:\n"
"                    session=n   ustaw numer <n> sesji,\n"
"                    mrl=m       dodaj mrl <m> do listy odtwarzania,\n"
"                    audio=c     wybierz ścieżkę dźwięku (<c>: 'next' lub "
"'prev'),\n"
"                    spu=c       wybierz ścieżkę napisów (<c>: 'next' lub "
"'prev'),\n"
"                    volume=v    ustaw głośność dźwięku,\n"
"                    amp=v       ustaw poziom wzmocnienia,\n"
"                    loop=m      ustaw tryb powtarzania (<m>: 'none' 'loop' "
"'repeat' 'shuffle' lub 'shuffle+'),\n"
"                    (playlist|pl)=p\n"
"                                 <p> może być:\n"
"                                   'clear'  wczyść listę odtwarzania,\n"
"                                   'first'  odtwarzaj pierwszą pozycję z "
"listy odtwarzania,\n"
"                                   'prev'   odtwarzaj poprzednią pozycję z "
"listy,\n"
"                                   'next'   odtwarzaj następną pozycję z "
"listy,\n"
"                                   'last'   odtwarzaj ostatnią pozycję z "
"listy,\n"
"                                   'load:s' ładuj listę odtwarzania z pliku "
"<s>,\n"
"                                   'stop'   zatrzymaj odtwarzanie po "
"zakończeniubierzącej pozycji.\n"
"                                   'cont'   kontynuuj odtwarzanie po "
"zakończeniu bierzącej pozycji.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"

#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           Nie rozpoczynaj automatycznie odtwarzania "
"(smart-mode).\n"

#, c-format
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""
"  -D, --deinterlace [post]...  Usuwanie przeplotu w oknie wyjścia obrazu. "
"Jedna lub więcej \n"
"                                 wtyczek post-przetwarzania może być "
"ustawiona za pomocą  parametru.\n"
"                                 Składnia taka sama jak w opcji --post \n"

#, c-format
msgid ""
"  -r, --aspect-ratio <mode>    Set aspect ratio of video output. Modes are:\n"
"                                 'auto', 'square', '4:3', 'anamorphic', "
"'dvb'.\n"
msgstr ""
"  -r, --aspect-ratio <mode>    Ustaw proporcje okna wyjścia obrazu. Tryby "
"to:\n"
"                                 'auto', 'square', '4:3', 'anamorphic',  "
"'dvb'.\n"

#, c-format
msgid ""
"      --broadcast-port <port>  Set port of xine broadcaster (master side)\n"
"                               Slave is started with 'xine slave://address:"
"port'\n"
msgstr ""
"      --broadcast-port <port>  Ustaw port transmisji xine (strona nadająca)\n"
"                               Stronę odbierającą xine startujemy z slave://"
"address:port'\n"

#, c-format
msgid "      --no-logo                Don't display the logo.\n"
msgstr "      --nologo                nie wyświetlaj logo.\n"

#, c-format
msgid "  -E, --no-reload              Don't reload old playlist.\n"
msgstr ""
"  -E, --no-reload              Nie ładuj ostatniej listy odtwarzania.\n"

#, c-format
msgid ""
"      --post <plugin>[:parameter=value][,...][;...]\n"
"                               Load one or many post plugin(s).\n"
"                               Parameters are comma separated.\n"
"                               This option can be used more than one time.\n"
msgstr ""
"      --post <plugin>[:parametr=wartość][,...][;...]\n"
"                               Ładuj jeden lub więcej wtyczek post-"
"przetwarzania.\n"
"                               Parametry oddzielone przecinkiem.\n"
"                               Ta opcja może być użyta więcej niż raz.\n"

#, c-format
msgid "      --disable-post           Don't enable post plugin(s).\n"
msgstr ""
"      --disable-post           Nie włączaj wtyczek post-przetwarzania.\n"

#, c-format
msgid "      --no-splash              Don't display the splash screen.\n"
msgstr "      --no-splash              Nie pokazuj ekranu powitalnego.\n"

#, c-format
msgid ""
"      --stdctl                 Control xine by the console, using lirc "
"commands.\n"
msgstr ""
"      --stdctl                 Kontroluj xine przez konsolę używając komend "
"lirc.\n"

#, c-format
msgid "  -T, --tvout <backend>        Turn on tvout support. Backend can be:\n"
msgstr ""
"  -T, --tvout <backend>        Wącz obsługę dla wyjścia TV. <backend> może "
"być:\n"

#, c-format
msgid ""
"      --list-plugins [=type]   Display the list of all available plugins,\n"
"                               Optional <type> can be:\n"
"                    audio_out, video_out, demux, input, sub, post,\n"
"                    audio_decoder, video_decoder.\n"
msgstr ""
"      --list-plugins [=type]   Wyświetl wszystkie dostępne wtyczki,\n"
"                               Opcjonalnie <type> może być:\n"
"                    audio_out, video_out, demux, input, sub, post,\n"
"                    audio_decoder, video_decoder.\n"

#, c-format
msgid ""
"      --bug-report [=mrl]      Enable bug report mode:\n"
"                                 Turn on verbosity, gather all output\n"
"                                 messages and write them into a file named\n"
"                                 BUG-REPORT.TXT.\n"
"                                 If <mrl> is given, xine will play the mrl\n"
"                                 then quit (like -pq).\n"
msgstr ""
"      --bug-report [=mrl]      Włącz tryb raportowania błędów:\n"
"                                 Włącz 'gadatliwość', zbieraj wszystkie "
"komunikaty\n"
"                                 i zapisuj je do pliku nazwanego\n"
"                                 BUG-REPORT.TXT.\n"
"                                 Jeśli podany zostanie <mrl>, xine odtworzy "
"go\n"
"                                 i zakończy się (tak jak -pq).\n"

