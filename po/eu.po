#: src/xitk/main.c:584
#, c-format
msgid "Usage: xine [OPTIONS]... [MRL]\n"
msgstr "Erabilera: xine [AUKERAK]... [MRL]\n"

#: src/xitk/main.c:587
#, c-format
msgid "  -v, --version                Display version.\n"
msgstr "  -v, --version                Bistaratu bertsioa.\n"

#: src/xitk/main.c:588
#, c-format
msgid "      --verbose [=level]       Set verbosity level. Default is 1.\n"
msgstr ""
"      --verbose [=maila]       Ezarri xehetasun-maila. Lehenetsia 1 da.\n"

#: src/xitk/main.c:589
#, c-format
msgid ""
"  -c, --config <file>          Use config file instead of default one.\n"
msgstr ""
"  -c, --config <file>          Erabili config fitxategia lehenetsiaren "
"ordez.\n"

#: src/xitk/main.c:590
#, c-format
msgid ""
"  -V, --video-driver <drv>     Select video driver by id. Available "
"drivers: \n"
msgstr ""
"  -V, --video-driver <drv>     Hautatu bideo-kontrolatzailea id bidez. "
"Erabilgarri dauden kontrolatzaileak:\n"

#: src/xitk/main.c:601
#, c-format
msgid ""
"  -A, --audio-driver <drv>     Select audio driver by id. Available "
"drivers: \n"
msgstr ""
"  -A, --audio-driver <drv>     Hautatu audio-kontrolatzailea id bidez. "
"Erabilgarri dauden kontrolatzaileak:\n"

#: src/xitk/main.c:611
#, c-format
msgid "  -u, --spu-channel <#>        Select SPU (subtitle) channel '#'.\n"
msgstr "  -u, --spu-channel <#>        Hautatu '#' SPU (azpititulua) kanala.\n"

#: src/xitk/main.c:612
#, c-format
msgid "  -a, --audio-channel <#>      Select audio channel '#'.\n"
msgstr "-a, --audio-channel <#> Hautatu '#' audio-kanala.\n"

#: src/xitk/main.c:613
#, c-format
msgid ""
"  -p, --auto-play [opt]        Play on start. Can be followed by:\n"
"                    'f': in fullscreen mode.\n"
"                    'h': hide GUI (panel, etc.).\n"
"                    'w': hide video window.\n"
"                    'q': quit when play is done.\n"
"                    'd': retrieve playlist from DVD. (deprecated. use -s "
"DVD)\n"
"                    'v': retrieve playlist from VCD. (deprecated. use -s "
"VCD)\n"
msgstr ""
"  -p, --auto-play [opt]        Abiaraztean erreproduzitu. Hauekin erabili "
"daiteke:\n"
"                    'f': pantaila oso moduan.\n"
"                    'h': GUI ezkutatu (panela, etab.).\n"
"                    'w': bideo leihoa ezkutatu.\n"
"                    'q': Itxi erreprodukzioa amaitzean.\n"
"                    'd': erreprodukzio zerrenda DVD-tik jaso. (gaitzetsia. -"
"s DVD erabili)\n"
"                    'v': erreprodukzio zerrenda VCD-tik jaso. (gaitzetsia. -"
"s VCD erabili)\n"

#: src/xitk/main.c:621
#, c-format
msgid "                    'F': in xinerama fullscreen mode.\n"
msgstr "                    'F': xinerama pantaila osoko moduan.\n"

#: src/xitk/main.c:623
#, c-format
msgid "  -s, --auto-scan <plugin>     auto-scan play list from <plugin>\n"
msgstr ""
"  -s, --auto-scan <plugin>     eskaneatu automatikoki erreprodukzio-zerrenda "
"<plugin>etik\n"

#: src/xitk/main.c:624
#, c-format
msgid "  -f, --fullscreen             start in fullscreen mode,\n"
msgstr "  -f, --pantaila osoa             hasi pantaila osoko moduan,\n"

#: src/xitk/main.c:626
#, c-format
msgid ""
"  -F, --xineramafull           start in xinerama fullscreen (display on "
"several screens),\n"
msgstr ""
"  -F, --xineramafull           hasi xinerama pantaila osoan (bistaratu "
"hainbat pantailatan),\n"

#: src/xitk/main.c:628
#, c-format
msgid "  -g, --hide-gui               hide GUI (panel, etc.)\n"
msgstr "  -g, --hide-gui               ezkutatu GUI (panela, etab.)\n"

#: src/xitk/main.c:629
#, c-format
msgid "  -I, --no-gui                 disable GUI\n"
msgstr "-I, --no-gui GUI ezgaitu\n"

#: src/xitk/main.c:630
#, c-format
msgid "  -H, --hide-video             hide video window\n"
msgstr "  -H, --hide-video             ezkutatu bideoaren leihoa\n"

#: src/xitk/main.c:632
#, c-format
msgid "  -L, --no-lirc                Turn off LIRC support.\n"
msgstr "  -L, ---no-lirc                Desaktibatu LIRC euskarria.\n"

#: src/xitk/main.c:634
#, c-format
msgid ""
"      --visual <class-or-id>   Use the specified X11 visual. <class-or-id>\n"
"                               is either an X11 visual class, or a visual "
"id.\n"
msgstr ""
"      --visual <class-or-id>   Ezarritako X11 bisual-a erabili. <class-or-"
"id>\n"
"                               X11 bisual klase bat edo bisual id bat da.\n"

#: src/xitk/main.c:636
#, c-format
msgid "      --install                Install a private colormap.\n"
msgstr "      --instalatu                Instalatu kolore-mapa pribatu bat.\n"

#: src/xitk/main.c:637
#, c-format
msgid ""
"      --keymap [=option]       Display keymap. Option are:\n"
"                                 'default': display default keymap table,\n"
"                                 'lirc': display draft of a .lircrc config "
"file.\n"
"                                 'remapped': user remapped keymap table.\n"
"                                 'file:<filename>': use <filename> as "
"keymap.\n"
"                                 -if no option is given, 'default' is "
"selected.\n"
msgstr ""
"      --keymap [=aukera]       Teklatu-mapa bistaratu. Aukerak:\n"
"                                 'default': lehenetsitako teklatu-mapa "
"bistaratu,\n"
"                                 'lirc': .lircrc configurazio zirriborro bat "
"bistaratzen du.\n"
"                                 'remapped': erabiltzaileak moldatutako "
"teklatu-mapa taula.\n"
"                                 'file:<filename>': erabili <filename> "
"teklatu-mapa bezala.\n"
"                                 -aukerarik ez eman ezkero, 'lehenetsirikoa' "
"erabiliko da.\n"

#: src/xitk/main.c:643
#, c-format
msgid "  -n, --network                Enable network remote control server.\n"
msgstr ""
"  -n, --network                Gaitu sarearen urruneko kontrolaren "
"zerbitzaria.\n"

#: src/xitk/main.c:644
#, c-format
msgid "      --network-port           Specify network server port number.\n"
msgstr ""
"      --network-port           Sare zerbitzari ataka zenbakia ezarri.\n"

#: src/xitk/main.c:645
#, c-format
msgid "  -R, --root                   Use root window as video window.\n"
msgstr ""
"  -R, --root                   Erabili erroko leihoa bideoaren leiho gisa.\n"

#: src/xitk/main.c:646
#, c-format
msgid "  -G, --geometry <WxH[+X+Y]>   Set output window geometry (X style).\n"
msgstr ""
"  -G, --geometria <WxH[+X+Y]>   Ezarri irteerako leihoaren geometria (X "
"estiloa).\n"

#: src/xitk/main.c:647
#, c-format
msgid "  -B, --borderless             Borderless video output window.\n"
msgstr ""
"  -B, --borderless             Ertzik gabeko bideoaren irteera-leihoa.\n"

#: src/xitk/main.c:648
#, c-format
msgid ""
"  -N, --animation <mrl>        Specify mrl to play when video output isn't "
"used.\n"
"                                 -can be used more than one time.\n"
msgstr ""
"  -N, --animation <mrl>        Adierazi erreproduzitu beharreko mrl-a bideo-"
"irteera erabiltzen ez denean.\n"
"                                 -bein baino gehiago erabili daiteke.\n"

#: src/xitk/main.c:650
#, c-format
msgid "  -P, --playlist <filename>    Load a playlist file.\n"
msgstr ""
"  -P, --playlist <fitxategi-izena>    Kargatu erreprodukzio--zerrendaren "
"fitxategia.\n"

#: src/xitk/main.c:651
#, c-format
msgid ""
"  -l, --loop [=mode]           Set playlist loop mode (default: loop). Modes "
"are:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"
msgstr ""
"  -l, --loop [=mode]           Ezarri erreprodkzui zerrenda errepikapena "
"(lehenespena: errepikatu). Moduak:\n"
"                                 'loop': loop entire playlist.\n"
"                                 'repeat': repeat current playlist entry.\n"
"                                 'shuffle': select randomly a yet unplayed "
"entry from playlist\n"
"                                 'shuffle+': same as 'shuffle', but "
"indefinetely replay the playlist.\n"

#: src/xitk/main.c:656
#, c-format
msgid "      --skin-server-url <url>  Define the skin server url.\n"
msgstr "      --skin-server-url <url>  Definitu azal-zerbitzariaren url-a.\n"

#: src/xitk/main.c:657
#, c-format
msgid ""
"      --enqueue <mrl> ...      Enqueue mrl of a running session (session 0)\n"
msgstr ""
"--enqueue <mrl> ...Gehitu martxan dagoen saio baten mrl-a ilaran (0 saioa)\n"

#: src/xitk/main.c:658
#, c-format
msgid ""
"  -S, --session <option1,option2,...>\n"
"                               Session managements. Options can be:\n"
"                    session=n   specify session <n> number,\n"
"                    mrl=m       add mrl <m> to the playlist,\n"
"                    audio=c     select audio channel (<c>: 'next' or "
"'prev'),\n"
"                    spu=c       select spu channel (<c>: 'next' or 'prev'),\n"
"                    volume=v    set audio volume,\n"
"                    amp=v       set amplification level,\n"
"                    loop=m      set loop mode (<m>: 'none' 'loop' 'repeat' "
"'shuffle' or 'shuffle+'),\n"
"                    get_speed   get speed value (see man page for returned "
"value).\n"
"                    (playlist|pl)=p\n"
"                                 <p> can be:\n"
"                                   'clear'  clear the playlist,\n"
"                                   'first'  play first entry in the "
"playlist,\n"
"                                   'prev'   play previous playlist entry,\n"
"                                   'next'   play next playlist entry,\n"
"                                   'last'   play last entry in the "
"playlist,\n"
"                                   'load:s' load playlist file <s>,\n"
"                                   'stop'   stop playback at the end of "
"current playback.\n"
"                                   'cont'   continue playback at the end of "
"current playback.\n"
"                    play, slow2, slow4, pause, fast2,\n"
"                    fast4, stop, quit, fullscreen, eject.\n"
msgstr ""
"-S, --session <1aukera,2aukera,...>\n"
"Saio kudeatzaileak. Aukerak:\n"
"session=n   ezarri <n> saio zenbakia,\n"
"mrl=m gehitu <m> mrl erreprodukzioa zerrendara,\n"
"audio=c aukeratu audio kanala (<c>: 'next' edo 'prev'),\n"
"spu=c aukeratu spu kanala (<c>: 'next' edo 'prev'),\n"
"volume=v ezarri audio bolumena,\n"
"anp=v anplifikazio maila ezarri,\n"
"loop=m errepikatze modua ezarri (<m>: 'none' 'loop' 'repeat' 'shuffle' edo "
"'shuffle+'),\n"
"get_speed abiadura balioa eskuratu (begiratu manual orria itzuklitako balioa "
"ulertzeko)\n"
"(playlist|pl)=p\n"
"<p> aukerak:\n"
"'clear'  zerrenda garbitu,\n"
"'first'  Zerrendako lehen sarrera erreproduzitu,\n"
"'prev' Zerrendako aurreko sarrera erreproduzitu,\n"
"'next' Zerrendako hurrengo sarrera erreproduzitu,\n"
"'last' Zerrendako azken sarrera erreproduzitu,\n"
"'load:s' <s> fitxategiko zerrenda kargatu,\n"
"'stop' erreprodukzioa utzi unekoa amaizterakoan.\n"
"'cont' jarraitu erreprodukzioa zerrenda amaiatu arte.\n"
"play, slow2, slow4, pause, fast2,\n"
"fast4, stop, quit, fullscreen, eject.\n"

#: src/xitk/main.c:680
#, c-format
msgid ""
"  -Z                           Don't automatically start playback (smart "
"mode).\n"
msgstr ""
"  -Z                           Ez hasi automatikoki erreproduzitzen (modu "
"dotorea).\n"

#: src/xitk/main.c:681
#, c-format
msgid ""
"  -D, --deinterlace [post]...  Deinterlace video output. One or more post "
"plugin\n"
"                                 can be specified, with optional "
"parameters.\n"
"                                 Syntax is the same as --post option.\n"
msgstr ""

#: src/xitk/main.c:684
